{
  "name": "muffin-cli",
  "description": "The command line utility for building sites using muffin",
  "version": "0.3.0",
  "author": "leo",
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp transpile",
    "test": "eslint bin/** lib/**"
  },
  "eslintConfig": {
    "extends": "default",
    "parser": "babel-eslint"
  },
  "license": "MIT",
  "preferGlobal": true,
  "repository": "muffin/cli",
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "syntax-async-functions",
      "transform-es2015-modules-commonjs"
    ]
  },
  "bin": {
    "muffin": "dist/bin/index",
    "muffin-new": "dist/bin/new",
    "muffin-serve": "dist/bin/serve",
    "muffin-build": "dist/bin/build",
    "muffin-import": "dist/bin/import"
  },
  "bugs": {
    "url": "https://github.com/muffin/cli/issues"
  },
  "dependencies": {
    "args": "^1.1.0",
    "broccoli": "^0.16.9",
    "broccoli-sane-watcher": "^1.1.4",
    "chalk": "^1.1.3",
    "dotenv": "^2.0.0",
    "fs-extra": "^0.30.0",
    "git-config": "0.0.7",
    "inquirer": "^1.0.1",
    "mongonaut": "^2.1.0",
    "mongoose": "^4.4.19",
    "ncp": "^2.0.0",
    "ora": "^0.2.3",
    "resolve": "^1.1.7",
    "update-notifier": "^0.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "eslint": "^2.10.2",
    "eslint-config-default": "^0.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0",
    "gulp-chmod": "^1.3.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "data",
    "template",
    "dist"
  ],
  "greenkeeper": {
    "emails": false
  },
  "homepage": "http://muffin.cafe",
  "keywords": [
    "muffin",
    "command",
    "line",
    "utility",
    "cms",
    "content",
    "management",
    "system"
  ]
}
