{
  "name": "muffin",
  "version": "1.1.0",
  "description": "The 21st century way of building websites",
  "preferGlobal": true,
  "main": "index.js",
  "scripts": {
    "start": "nodemon index -i dist"
  },
  "files": [
    "dist",
    "index.js",
    "lib",
    "models",
    "routes"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/small-cake/core.git"
  },
  "bin": {
    "muffin": "index.js",
    "muffin-init": "bin/init.js"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "keywords": [
    "cms",
    "content",
    "management",
    "system",
    "chocolate"
  ],
  "greenkeeper": {
    "emails": false,
    "ignore": [
      "koa-mount",
      "koa-sendfile"
    ]
  },
  "author": "Leonard Lamprecht <mindrun@icloud.com> (http://leo.im)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/small-cake/core/issues"
  },
  "homepage": "http://muffin.cafe",
  "dependencies": {
    "bcrypt": "^0.8.5",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "dotenv": "^2.0.0",
    "fs-extra": "^0.26.7",
    "gridfs-stream": "^1.1.1",
    "koa": "^1.2.0",
    "koa-body": "^1.4.0",
    "koa-compress": "^1.0.9",
    "koa-handlebars": "^0.5.7",
    "koa-jwt": "^1.1.2",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-sendfile": "^1.1.1",
    "koa-static": "^2.0.0",
    "mongoose": "^4.4.10",
    "query-string": "^4.0.2",
    "update-notifier": "^0.6.3",
    "walk": "^2.3.9"
  },
  "devDependencies": {
    "nodemon": "^1.9.1"
  }
}
