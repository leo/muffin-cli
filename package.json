{
  "name": "muffin-cli",
  "description": "The command line utility for building sites using muffin",
  "version": "0.2.0",
  "author": "leo",
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp transpile"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator",
      "syntax-async-functions"
    ]
  },
  "bin": {
    "muffin": "dist/bin/index",
    "muffin-new": "dist/bin/new",
    "muffin-serve": "dist/bin/serve",
    "muffin-build": "dist/bin/build",
    "muffin-import": "dist/bin/import"
  },
  "bugs": {
    "url": "https://github.com/muffinjs/cli/issues"
  },
  "dependencies": {
    "args": "^1.0.1",
    "broccoli": "^0.16.9",
    "broccoli-sane-watcher": "^1.1.4",
    "chalk": "^1.1.3",
    "fs-extra": "^0.30.0",
    "git-config": "0.0.7",
    "inquirer": "^1.0.1",
    "mongonaut": "^2.1.0",
    "mongoose": "^4.4.16",
    "ncp": "^2.0.0",
    "ora": "^0.2.1",
    "resolve": "^1.1.7",
    "update-notifier": "^0.7.0"
  },
  "devDependencies": {
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0",
    "gulp-chmod": "^1.3.0",
    "gulp-ext": "^1.0.0"
  },
  "files": [
    "data",
    "template",
    "dist"
  ],
  "greenkeeper": {
    "emails": false
  },
  "homepage": "http://muffin.cafe",
  "keywords": [
    "muffin",
    "command",
    "line",
    "utility",
    "cms",
    "content",
    "management",
    "system"
  ],
  "license": "MIT",
  "preferGlobal": true,
  "repository": "muffinjs/cli"
}
